<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "cpf.dtd">
<properties>
  <simple name="minDeltaVel" type="double"><description>Minimun velocity variation that will produce a new trajectory generation</description><value>0.001</value></simple>
  <simple name="goalDistanceThreeshold" type="double"><description>The min distance between the current pose and the target pose for considering the transaltion as done</description><value>0.1</value></simple>
